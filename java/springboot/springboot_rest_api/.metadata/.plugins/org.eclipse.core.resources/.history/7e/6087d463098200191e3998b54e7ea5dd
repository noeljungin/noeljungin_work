package hello;

import com.fasterxml.jackson.annotation.JsonIgnoreProperties;
import com.fasterxml.jackson.annotation.JsonIgnoreProperties.Value;

@JsonIgnoreProperties(ignoreUnknown = true)
public class Quote {
	
	private String type;
	private Value value;
	
	public Quote() {
	}
	
	public String getType() {
		return type;
	}

}
